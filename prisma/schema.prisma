generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  email     String   @unique
  name      String?
}

model Ulasan {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  nama      String
  rating    Int
  komentar  String
  kendaraan String
}
model Galery {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  imageUrl  String
  caption   String
}